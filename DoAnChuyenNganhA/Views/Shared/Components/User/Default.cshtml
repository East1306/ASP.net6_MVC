@model List<DoAnChuyenNganhA.Models.NhanVien>
@{
    var sum = 0;
}

@foreach (var s in Model)
{
    sum++;
}

<div class="col-lg-3 col-6">
    <!-- small box -->
    <div class="small-box bg-warning">
        <div class="inner">
            <h3>@sum</h3>
            <p>Staff</p>
        </div>
        <div class="icon">
            <i class="ion ion-person-add"></i>
        </div>
        <a href="~/staff/user" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
    </div>
</div>